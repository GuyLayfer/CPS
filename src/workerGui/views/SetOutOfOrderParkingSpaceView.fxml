<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="workerGui.controllers.SetOutOfOrderParkingSpaceController">
	<children>
		<Text layoutX="100.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Set out of order parking space">
			<font>
				<Font name="System Bold" size="14.0" />
			</font>
		</Text>
		<Text layoutX="25.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Parking Lot ID:" />
		<Text layoutX="25.0" layoutY="125.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Floor:" />
		<Text layoutX="25.0" layoutY="165.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Row:" />
		<Text layoutX="25.0" layoutY="205.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Column:" />
		<ComboBox fx:id="ParkingLotId" layoutX="140.0" layoutY="70.0" prefHeight="25.0" prefWidth="150.0" />
		<ComboBox fx:id="LotFloorComboBox" layoutX="140.0" layoutY="110.0" prefWidth="150.0" />
		<ComboBox fx:id="LotRowComboBox" layoutX="140.0" layoutY="150.0" prefWidth="150.0" />
		<ComboBox fx:id="LotColumnComboBox" layoutX="140.0" layoutY="190.0" prefWidth="150.0" />
		<Button fx:id="SetOutOfOrderButton" layoutX="100.0" layoutY="240.0" onAction="#setOutOfOrderParkingSpace" text="Set Out Of Order">
			<font>
				<Font size="14.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
